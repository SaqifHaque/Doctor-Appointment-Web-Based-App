<!DOCTYPE html>
<html>

<head>
	<title>Add Receptionist</title>
</head>

<body>
	<h2>Add</h2>
	<a href="/admin">Home</a> |
	<a href="/logout">Logout</a><br><br>
	<a href="/admin/profile">Edit Profile</a><br>
	<a href="/admin/addNewDoctor">Add New Doctor</a><br>
	
	<a href="/admin/view_Doctor">Doctor</a><br>
	
	
	
	

	<form method="post">
		<table border="1">
			
			<tr>
				<td>
				Id:
			</td>
			<td> <input type="text" name="id" value="<%= user.id %>"required></td>
		</tr>
				<tr>
					<td>
					Username:
				</td>
				<td> <input type="text" name="username" value="<%= user.username %>" required></td>
			</tr>
			
		
			<tr>
				<td>
					Email :
				</td>
				<td><input type="text" name="email" value="<%= user.email %>" required></td>
			</tr>
			<tr>
				<td>
					Blood Group:
				</td>
				<td><input type="text" name="bloodgroup" value="<%= user.bloodgroup%>" required></td>
			</tr>
			<tr>
				<td>
					Phone :
				</td>
				<td><input type="text" name="phonenumber" value="<%= user.phonenumber %>" required></td>
			</tr>
			<tr>
				<td>
					Department:
				</td>
				<td><input type="text" name="dept" value="<%= user.dept %>" required></td>
            </tr>
            <tr>
				<td>
					Password:
				</td>
				<td><input type="password" name="password" value="<%= user.password%>"required></td>
			</tr>
			<tr>
				<td>
					Confirm Password:
				</td>
				<td><input type="password" name="cpassword" value="<%= user.password%>"required></td>
			</tr>

		</table>

		<input type="Submit" name="" value="Submit"><br>

	</form>
</body>
</html>